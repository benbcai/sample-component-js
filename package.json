{
  "name": "sample-component-js",
  "version": "0.0.0",
  "description": "Node module to support the rails engine sample_component_engine",
  "engines": {
    "node": "^10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.cerner.com/orion/sample-component-js.git"
  },
  "bugs": {
    "url": "https://jira2.cerner.com/projects/UXPLATFORM"
  },
  "keywords": [
    "Cerner",
    "sample-component-js"
  ],
  "associated_engine": "git://github.cerner.com/orion/sample_component_engine.git",
  "publishConfig": {
    "registry": "https://cernerrepos.net/api/npm/npm"
  },
  "rollOut": {
    "TODO": "Remove this line but more info on roll-out here: https://pages.github.cerner.com/orion/orion-toolkit-js/#/tools/orion-toolkit-js/roll-out-script",
    "groupId": "com.cerner.ion",
    "artifactId": "sample-component-js",
    "site": {
      "repositoryId": "cerner-release-main-site",
      "repositoryUrl": "http://repo.release.cerner.corp/nexus/service/local/repositories/main-site"
    }
  },
  "author": "Cerner Corporation",
  "browserslist": [
    "extends @cerner/browserslist-config-terra"
  ],
  "eslintConfig": {
    "extends": "@cerner/terra"
  },
  "stylelint": {
    "extends": "stylelint-config-terra"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "rimraf ./package-lock.json && rimraf ./node_modules",
    "clean:install": "npm run clean && npm install",
    "clean:obsolete-jest-snapshots": "jest -u",
    "clean:obsolete-wdio-snapshots": "tt-clean-screenshots",
    "compile": "babel src --out-dir lib --copy-files",
    "compile:engine": "babel src --out-dir ../sample_component_engine/node_modules/sample-component-js/lib --copy-files",
    "deploy-gh-pages": "TERRA_DEV_SITE_PUBLIC_PATH='/orion/sample-component-js/' npm run pack && gh-pages -d build",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint src/**/*.scss --allow-empty-input",
    "pack": "NODE_ENV=production webpack -p",
    "postpublish": "npm run roll-out",
    "precompile": "rimraf lib",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "roll-out": "ot-roll-out",
    "start": "tt-serve",
    "start-prod": "tt-serve --env.disableHotReloading -p",
    "start-static": "npm run pack && tt-serve-static",
    "test": "npm run lint && npm run test:jest && npm run test:wdio",
    "test:jest": "jest",
    "test:lint": "npm run lint",
    "test:wdio": "wdio",
    "test:wdio:local": "tt-wdio --gridUrl='grid314.test.devcernerpowerchart.com' --locales=['en','en-AU','en-CA','en-GB','en-US','es','fr','nl','pt','sv']"
  },
  "dependencies": {
    "prop-types": "^15.5.8",
    "terra-action-header": "^2.0.0",
    "terra-content-container": "^3.0.0",
    "terra-spacer": "^3.0.0",
    "terra-status-view": "^4.0.0"
  },
  "peerDependencies": {
    "orion-application": "^1.1.0",
    "react-dom": "^16.8.5",
    "react-intl": "^2.7.2",
    "react": "^16.8.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "@cerner/browserslist-config-terra": "^3.0.0",
    "@cerner/eslint-config-terra": "^4.0.0",
    "core-js": "^3.1.3",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.2.2",
    "enzyme": "^3.3.0",
    "eslint": "^6.1.0",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "orion-application": "^1.1.0",
    "orion-dev-site-plugin": "^1.0.0",
    "orion-toolkit-js": "^4.1.0",
    "raf": "^3.4.1",
    "react-dom": "^16.8.5",
    "react": "^16.8.5",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^3.0.2",
    "stylelint-config-terra": "^3.4.0",
    "stylelint": "^11.0.0",
    "terra-aggregate-translations": "^1.8.0",
    "terra-dev-site": "^6.8.0",
    "terra-enzyme-intl": "^3.0.0",
    "terra-toolkit": "^6.0.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.1.2",
    "webpack": "^4.30.0"
  }
}
